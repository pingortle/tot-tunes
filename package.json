{
  "name": "tot-tunes",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "test": "teenytest test/lib/**/*.test.js",
    "test:deprecated": "KEYBOARD_DEVICE=sample-data.bin node index.js",
    "build": "pkg package.json -t node10-linux-armv7 --no-bytecode --out-path out",
    "deploy": "ssh pi@raspberrypi.local 'sudo systemctl stop tot-tunes.service' && scp out/tot-tunes pi@raspberrypi.local:~/ && ssh pi@raspberrypi.local 'sudo systemctl start tot-tunes.service'",
    "build-deploy": "npm run build && npm run deploy"
  },
  "author": "Kaleb Lape",
  "license": "MIT",
  "pkg": {
    "targets": [
      "node9-linux-armv7"
    ],
    "arch": "armv7",
    "nodeRange": "latest",
    "platform": "linux"
  },
  "teenytest": {
    "plugins": [
      "teenytest-promise"
    ]
  },
  "devDependencies": {
    "pkg": "^4.3.4",
    "prettier-standard": "^8.0.1",
    "standard": "^12.0.1",
    "teenytest": "^5.1.1",
    "teenytest-promise": "^1.0.0",
    "testdouble": "^3.9.2"
  },
  "dependencies": {
    "@cycle/run": "^5.2.0",
    "rxjs": "^5.5.8",
    "xstream": "^11.7.0"
  }
}
